<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_login_with_id"
        android:layout_width="403dp"
        android:layout_height="353dp"
        android:layout_marginTop="5dp"
        android:text="아이디로 로그인"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_login_with_fingerprint"
        android:layout_width="404dp"
        android:layout_height="360dp"
        android:layout_marginTop="5dp"
        android:text="지문으로 로그인"
        app:layout_constraintEnd_toEndOf="@+id/btn_login_with_id"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_login_with_id"
        app:layout_constraintTop_toBottomOf="@+id/btn_login_with_id" />
</androidx.constraintlayout.widget.ConstraintLayout>
